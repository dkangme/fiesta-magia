<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Media
 */
class Media
{
    /**
     * @var string
     */
    private $comments;

    /**
     * @var string
     */
    private $path;

    /**
     * @var integer
     */
    private $correlative;

    /**
     * @var boolean
     */
    private $front;

    /**
     * @var integer
     */
    private $idMedia;

    /**
     * @var \AppBundle\Entity\Event
     */
    private $idEvent;


    /**
     * Set comments
     *
     * @param string $comments
     * @return Media
     */
    public function setComments($comments)
    {
        $this->comments = $comments;

        return $this;
    }

    /**
     * Get comments
     *
     * @return string 
     */
    public function getComments()
    {
        return $this->comments;
    }

    /**
     * Set path
     *
     * @param string $path
     * @return Media
     */
    public function setPath($path)
    {
        $this->path = $path;

        return $this;
    }

    /**
     * Get path
     *
     * @return string 
     */
    public function getPath()
    {
        return $this->path;
    }

    /**
     * Set correlative
     *
     * @param integer $correlative
     * @return Media
     */
    public function setCorrelative($correlative)
    {
        $this->correlative = $correlative;

        return $this;
    }

    /**
     * Get correlative
     *
     * @return integer 
     */
    public function getCorrelative()
    {
        return $this->correlative;
    }

    /**
     * Set front
     *
     * @param boolean $front
     * @return Media
     */
    public function setFront($front)
    {
        $this->front = $front;

        return $this;
    }

    /**
     * Get front
     *
     * @return boolean 
     */
    public function getFront()
    {
        return $this->front;
    }

    /**
     * Set idMedia
     *
     * @param integer $idMedia
     * @return Media
     */
    public function setIdMedia($idMedia)
    {
        $this->idMedia = $idMedia;

        return $this;
    }

    /**
     * Get idMedia
     *
     * @return integer 
     */
    public function getIdMedia()
    {
        return $this->idMedia;
    }

    /**
     * Set idEvent
     *
     * @param \AppBundle\Entity\Event $idEvent
     * @return Media
     */
    public function setIdEvent(\AppBundle\Entity\Event $idEvent)
    {
        $this->idEvent = $idEvent;

        return $this;
    }

    /**
     * Get idEvent
     *
     * @return \AppBundle\Entity\Event 
     */
    public function getIdEvent()
    {
        return $this->idEvent;
    }
}
